# Comments shall be preceded by a # character
# All comments in this templante will not be present in the generated metadata file

adc_signal_carrier_frequency = 477999852 Hz                                         # if analog dowconversion is used, ADC signal carrier frequency should be the corresponding IF, otherwise it will equal rffe_signal_carrier_frequency
adc_signal_source = rffe                                                            # possible values: rffe, signal_generator
adc_clock_external_filter = none                                                    # possible values: none, "Minicircuits PLP-70+" / use colons if several filters are cascaded (e.g. "Minicircuits PLP-70+", "Minicircuits PLP-70+")
#beam_bunch_length = 11 ps
#beam_current = 250 mA
#beam_orbit_correction = off
#beam_pattern = multibunch                                                           # possible values: multibunch, singlebunch, twobunch, camshaft
#beam_pattern_mutibunch_fill = 300                                                   # expressed in term of filled buckets
#bpm_cables_delay_a = 0 ps                                                           # antenna A has been taken as reference, thus it has value 0
#bpm_cables_delay_b = 10 ps
#bpm_cables_delay_c = 5 ps
#bpm_cables_delay_d = 20 ps
dsp_sausaging = on
rffe_attenuators = 10 dB #, 15 dB                                                      # use colons to separate each attenuator value, where the first value indicate the first stage (from input to output) and so on
rffe_board_version = rffe_v2_476MHz                                                 # possible values: rffe_v1_476MHz, rffe_v2_476MHz, rffe_v1_500MHz, rffe_v2_500MHz
rffe_board_serialnumber = xxxxxxxxxxxx
rffe_signal_carrier_maxpower = -20 dBm                                              # maximum carrier signal power among A, B, C, D antennas
rffe_signal_source = signal_generator #beam                                                           # possible values: beam, signal_generator
rffe_switching = on

adc_board_serialnumber = xxxxxxxxxxxx
adc_board_version = fmc130_v1_passive_clock_distribution                            # possible values: fmc130_v1_passive_clock_distribution, fmc130_v1_active_clock_distribution_si571, fmc130_v1_active_clock_distribution_cvhd950
adc_clock_source = external
adc_clock_frequency_offset = 0 ppm
adc_clock_sampling_harmonic = 50                                                    # in relation to beam revolution harmonics
beam_harmonic_number = 363
#bpm_button_diameter = 6 mm
#bpm_button_gap = 0.3 mm
#bpm_button_thickness = 2 mm
#bpm_cables_model = LMR400                                                           # possible values: LMR195, LMR400
#bpm_name = AMP01A
#bpm_type = button_45deg                                                             # possible values: button_45deg, stripline_45deg, cavity
#bpm_geometry = circular                                                             # possible values: circular, ellipsoidal, octagonal
bpm_Kx = 10000000 mm
bpm_Ky = 10000000 mm
data_file_format = ascii
data_file_structure = bpm_amplitudes
data_general_description = Sirius BPM prototype tests at SPEAR3, March 2014
data_signature_method = md5                                                         # possible values: sha-1, sha-256, md5
data_source = experimental                                                          # possible values: experimental, experimental_postprocessed, simulation
dsp_deswitching_phase = 30 #210
dsp_firmware_version = XXXXXXXXXX
dsp_sausaging_window = tukey
dsp_sausaging_window_parameters = 1
#location = SPEAR3, SSRL, SLAC, Menlo Park, CA, USA
location_timezone = BRT #PST                                                        # possible values: PST, BRT
rffe_signal_carrier_frequency = 477999852 Hz
rffe_switching_phase = 0
rffe_switching_frequency_ratio = 1000
rffe_switching_frequency_reference = adc_clock                                      # possible values: beam_revolution_frequency, adc_clock
